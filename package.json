{
  "name": "master-chess",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "main": "./public/electron.js",
  "scripts": {
    "dev": "strapi develop",
    "start2": "concurrently -k \"npm run strapi\" \"npm:electron\"",
    "build": "strapi build -- --clean",
    "strapi": "strapi start",
    "adev": "npm run dev -- --watch-admin",
    "edev": "concurrently -k \"npm run strapi\" \"npm:electron\"",
    "electron": "wait-on tcp:1401 && electron .",
    "package-win": "electron-packager .  --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/uploads/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Master Chess\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=public/electron.js",
    "preelectron-pack": "strapi build",
    "start": "electron public/electron.js"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.3.0",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "electron-squirrel-startup": "^1.0.0",
    "html-pdf": "^3.0.1",
    "html-to-docx": "^1.2.2",
    "knex": "0.21.18",
    "moment": "^2.29.1",
    "sqlite3": "5.0.0",
    "strapi": "3.6.7",
    "strapi-admin": "3.6.7",
    "strapi-connector-bookshelf": "3.6.7",
    "strapi-helper-plugin": "^3.6.7",
    "strapi-plugin-content-manager": "3.6.7",
    "strapi-plugin-content-type-builder": "3.6.7",
    "strapi-plugin-email": "3.6.7",
    "strapi-plugin-entity-relationship-chart": "^3.1.0",
    "strapi-plugin-i18n": "3.6.7",
    "strapi-plugin-import-export-content": "^0.4.0",
    "strapi-plugin-upload": "3.6.7",
    "strapi-plugin-users-permissions": "3.6.7",
    "strapi-provider-email-nodemailer": "^3.6.7",
    "strapi-utils": "3.6.7"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "3cd721b5-2a95-42dc-9541-460dbbe510d8"
  },
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": "^6.0.0"
  },
  "license": "MIT"
}
